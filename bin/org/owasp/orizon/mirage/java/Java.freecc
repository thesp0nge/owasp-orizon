INJECT(PackageDeclaration) : {}
{
    public String getPackageName() {
        StringBuilder buf = new StringBuilder();
        boolean seenPackage = false;
        for (Token t : Nodes.getRealTokens(this)) {
            if (t.getId() == PACKAGE) {
                seenPackage = true;
            } else if (seenPackage) {
                if (t.getId() != SEMICOLON)
                    buf.append(t);
            }
        }
        return buf.toString();
    }
}

