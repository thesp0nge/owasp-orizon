INJECT(InterfaceDeclaration) :
{
}
{
    /**
     * Returns the interface's name.
     * 
     * @return the interface's name.
     */
	public String getName() {
        StringBuilder buf = new StringBuilder();
        // Since an interface is supposed to extends something
        // a WHITESPACE char is the delimiter between the name
        // and the extends clause.
        // 
        // FIXME: a malformed interface declaration can cause an
        // unpredictable behaviour here. Must fix in some way.
        for (Token tok : Nodes.getRealTokens(this)) {
            if (tok.getId() != INTERFACE)
                buf.append(tok);
            if (tok.getId() == EXTENDS)
            	break;
        }
        return buf.toString();
    }
}
ExtendsList ExtendsList(boolean isInterface) :
{
   boolean extendsMoreThanOne = false;
}
{
   "extends" ClassOrInterfaceType()
   ( "," ClassOrInterfaceType() { extendsMoreThanOne = true; } )*
   {
      if (extendsMoreThanOne && !isInterface)
         throw new ParseException("A class cannot extend more than one other class");
      return CURRENT_NODE;
   }
}

INJECT(ExtendsList) : 
{
     import java.util.*;
}
{
    public List<ObjectType> getTypes() {
        return Nodes.childrenOfType(this, ObjectType.class);
    }
    
    public void addType(ObjectType type, boolean isInterface) {
        if (!isInterface) {
            children = new java.util.ArrayList<Node>();
        }
        ListIterator<Node> iterator = iterator();
        if (!iterator.hasNext()) {
            iterator.add(Token.newToken(EXTENDS, "extends"));
            iterator.next();
            iterator.add(type);
        } else {
            while (iterator.hasNext()) {
                if (type.equals(iterator.next())) 
                    return;
            }
            iterator.add(Token.newToken(COMMA, ","));
            iterator.next();
            iterator.add(type);
        }
    }
}

